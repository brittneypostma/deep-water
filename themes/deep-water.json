    },
    {
      "name": "Dark Teal",
      "scope": [
        "string.template meta.template.expression",
        "keyword",
        "storage.type",
        "storage.modifier",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#01779b"
      }
    },
