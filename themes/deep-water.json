  "tokenColors": [
    {
      "name": "Italic",
      "scope": [
        "comment",
        "entity.name.type.class",
        "entity.other.attribute-name",
        "entity.name.method",
        "entity.name.tag",
        "keyword",
        "keyword.control",
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown",
        "storage",
        "string.other.link.title.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Gray",
      "scope": ["comment"],
      "settings": {
        "foreground": "#4b6869"
